            "id": "5719ea7a-1f21-41bd-a74c-3f1a0411e4e2",
            "data": {
              "type": "tm.Store",
              "name": "Database",
              "description": " persistent data layer",
              "outOfScope": false,
              "isTrustBoundary": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "isALog": false,
              "isEncrypted": false,
              "isSigned": false,
              "storesCredentials": false,
              "storesInventory": false,
              "threats": [
                {
                  "id": "0f1645ac-75d1-45fa-86a2-330192269815",
                  "title": "Connection string misuse / leaked DB credentials",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Spoofing",
                  "description": "Hard-coded or poorly stored DB credentials are exposed and used to connect as a trusted client.",
                  "mitigation": "Use a secrets manager; rotate credentials; prefer IAM/short-lived auth; restrict access to secrets.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 21,
                  "score": ""
                },
                {
                  "id": "e5ba69fb-ccd8-4565-9bfa-7b1cfac54255",
                  "title": "Direct data manipulation via SQL injection",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Tampering",
                  "description": "Unparameterized queries allow attackers to modify/insert/delete data directly in the database.",
                  "mitigation": "Parameterize queries; least-privilege DB accounts; WAF; input validation and prepared statements.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 22,
                  "score": ""
                },
                {
                  "id": "16d73a96-fa35-41cd-b23a-98e72b6ab291",
                  "title": "No query / access audit trail",
                  "status": "Open",
                  "severity": "High",
                  "type": "Repudiation",
                  "description": "Queries and data changes are not logged; insiders/attackers can alter data without trace.",
                  "mitigation": "Enable DB audit logging (DML/DDL); centralize logs; protect integrity; alert on suspicious patterns.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 23,
                  "score": ""
                },
                {
                  "id": "30150bbe-9ed0-49a3-b251-7e6df5fc4e2c",
                  "title": "Data leak from unencrypted backups",
                  "status": "Open",
                  "severity": "High",
                  "type": "Information Disclosure",
                  "description": "Backups stored unencrypted or in insecure locations expose sensitive data if accessed.",
                  "mitigation": "Encrypt backups at rest and in transit; restrict access; key rotation; secure disposal of old backups.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 24,
                  "score": ""
                },
                {
                  "id": "f1e2b9ec-92af-48d5-88fa-1aac0fa1a8f9",
                  "title": "Resource exhaustion / DB lock flooding",
                  "status": "Open",
                  "severity": "High",
                  "type": "Denial of Service",
                  "description": "Malicious or heavy queries cause lock contention, connection saturation, or buffer exhaustion.",
                  "mitigation": "Limit connection pools; query timeouts; monitor slow queries; resource quotas; DB-level rate limits.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 25,
                  "score": ""
                },
                {
                  "id": "4acb683c-c86e-48eb-b81c-2f8f930ec38f",
                  "title": "Privilege escalation via weak DB roles",
                  "status": "Open",
                  "severity": "High",
                  "type": "Elevation of Privilege",
                  "description": "Over-privileged roles or poor separation allow escalation to administrative privileges.",
                  "mitigation": "Least privilege; strict RBAC; separate app vs admin roles; periodic privilege reviews.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 26,
                  "score": ""
                }
              ],
              "threatFrequency": {
                "spoofing": 1,
                "tampering": 1,
                "repudiation": 1,
                "informationDisclosure": 1,
                "denialOfService": 1,
                "elevationOfPrivilege": 1
              }
            }
