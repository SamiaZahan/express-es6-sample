            "data": {
              "type": "tm.Process",
              "name": "Web Application",
              "description": "handles business logic",
              "outOfScope": false,
              "isTrustBoundary": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "handlesCardPayment": false,
              "handlesGoodsOrServices": false,
              "isWebApplication": false,
              "privilegeLevel": "",
              "threats": [
                {
                  "id": "32a54f0e-8ca3-4033-bfc2-a3b1d98faa03",
                  "title": "Fake or stolen JWT / session token accepted",
                  "status": "Open",
                  "severity": "High",
                  "type": "Spoofing",
                  "description": "Forged or replayed tokens allow attackers to impersonate users if signatures/claims/expiry are not validated.",
                  "mitigation": "Validate signatures and claims; enforce expiry and revocation; use short-lived tokens and strong signing keys.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 15,
                  "score": ""
                },
                {
                  "id": "f1988412-8a1b-45a9-baba-077de3c3f1e2",
                  "title": "Parameter tampering / SQL injection",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Tampering",
                  "description": "Manipulated parameters or inputs lead to SQL injection or altering of server behavior/data.",
                  "mitigation": "Use parameterized queries/ORM; rigorous server-side validation; schema validation; WAF rules for injection.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 16,
                  "score": ""
                },
                {
                  "id": "89b04aaf-b599-4f17-8641-55d6a8d14584",
                  "title": "Lack of audit logs (repudiation risk)",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Repudiation",
                  "description": "Important server-side actions lack secure, centralized logging, enabling users to deny actions.",
                  "mitigation": "Centralized, immutable logging with actor identity and timestamps; protect logs; regular review and alerting.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 17,
                  "score": ""
                },
                {
                  "id": "d069036d-ea6b-451b-88d3-654ad16637aa",
                  "title": "Sensitive error messages reveal stack trace",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Information Disclosure",
                  "description": "Detailed exceptions and stack traces are returned to clients, exposing implementation details or secrets.",
                  "mitigation": "Return generic errors to clients; log full detail server-side; disable debug in production.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 18,
                  "score": ""
                },
                {
                  "id": "2650735c-64d2-42a7-8a14-f01c3da36e85",
                  "title": "Excessive requests cause server crash",
                  "status": "Open",
                  "severity": "High",
                  "type": "Denial of Service",
                  "description": "High-volume or expensive requests exhaust CPU/memory/threads and degrade availability.",
                  "mitigation": "Server-side throttling/rate limiting; circuit breakers; autoscaling; WAF/edge caching.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 19,
                  "score": ""
                },
                {
                  "id": "6f930c0d-e3a4-483b-9974-d080027708a9",
                  "title": "Insecure code execution (shell injection / unsafe eval)",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Elevation of Privilege",
                  "description": "Unsafely executing user-controlled input leads to command injection or arbitrary code execution.",
                  "mitigation": "Avoid exec/eval on user input; use safe libraries; least-privilege runtime; static/dynamic code scans.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 20,
                  "score": ""
                }
              ],
              "threatFrequency": {
                "spoofing": 1,
                "tampering": 1,
                "repudiation": 1,
                "informationDisclosure": 1,
                "denialOfService": 1,
                "elevationOfPrivilege": 1
              }
            }
