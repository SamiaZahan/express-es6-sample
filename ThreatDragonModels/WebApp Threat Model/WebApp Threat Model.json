            "data": {
              "type": "tm.Actor",
              "name": "UI",
              "description": " represents browser/client",
              "isTrustBoundary": false,
              "outOfScope": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "providesAuthentication": false,
              "threats": [
                {
                  "id": "03cfdda4-bab6-46a4-a33f-42d906973888",
                  "title": "Phishing or session hijacking via stolen cookies",
                  "status": "Open",
                  "severity": "High",
                  "type": "Spoofing",
                  "description": "Attacker obtains credentials or steals session cookies (e.g., via phishing, mixed content, or insecure cookie flags) to impersonate a user.",
                  "mitigation": "Use Secure/HttpOnly/SameSite cookie flags; enforce MFA; shorten session lifetime; enable anti-phishing headers and training.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 9,
                  "score": ""
                },
                {
                  "id": "584059be-e211-419b-9202-9bc5dfce9abf",
                  "title": "Client-side script modification (malicious JS)",
                  "status": "Open",
                  "severity": "High",
                  "type": "Tampering",
                  "description": "Injected or compromised client-side scripts modify form inputs or UI actions, tampering with data sent to the server.",
                  "mitigation": "Enforce CSP, Subresource Integrity (SRI), avoid inline scripts; validate on server; restrict third-party scripts.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 10,
                  "score": ""
                },
                {
                  "id": "b91dd88d-7ea6-48e2-a72c-5418349cca96",
                  "title": "Users deny malicious input actions (no non-repudiation)",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Repudiation",
                  "description": "Actions initiated from the UI are not logged with sufficient integrity to prove who did what and when.",
                  "mitigation": "Create tamper-evident, append-only server-side audit logs with timestamps, user IDs; protect logs with access controls.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 11,
                  "score": ""
                },
                {
                  "id": "94da07f8-0c51-447a-aeb0-5507950efb43",
                  "title": "Exposure of credentials in browser cache / storage",
                  "status": "Open",
                  "severity": "High",
                  "type": "Information Disclosure",
                  "description": "Tokens/credentials stored in localStorage/sessionStorage or cached responses may be read by other scripts or local attackers.",
                  "mitigation": "Avoid storing secrets client-side; use HttpOnly cookies for tokens; prevent caching of sensitive endpoints; clear on logout.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 12,
                  "score": ""
                },
                {
                  "id": "89439676-e9a1-4f37-8c23-a4308a11fc35",
                  "title": "UI overload via repeated automated requests",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Denial of Service",
                  "description": "Automated clients/bots repeatedly trigger UI actions causing server or client-side resource exhaustion.",
                  "mitigation": "Use CAPTCHA on risky flows, enforce server-side rate limiting and WAF rules; implement bot detection.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 13,
                  "score": ""
                },
                {
                  "id": "83ef6360-5531-46ed-99b6-2261b6c41b44",
                  "title": "Cross-site scripting (XSS) used to run admin commands",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Elevation of Privilege",
                  "description": "Stored/reflected XSS executes in an admin\u2019s browser, performing privileged actions under their session.",
                  "mitigation": "Output encode; strict CSP; HttpOnly cookies; require step-up auth (MFA/re-auth) for critical admin actions.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 14,
                  "score": ""
                }
              ],
              "threatFrequency": {
                "spoofing": 1,
                "tampering": 1,
                "repudiation": 1,
                "informationDisclosure": 1,
                "denialOfService": 1,
                "elevationOfPrivilege": 1
              }
            },
